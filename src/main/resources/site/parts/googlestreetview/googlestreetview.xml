<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
	<display-name>Google street view</display-name>
  	<config>
		<input name="name" type="TextLine">
  			<label>Location Name</label>
	      	<occurrences minimum="1" maximum="1"/>
	    </input>
  		<input name="coordinates" type="GeoPoint">
	      	<label>GPS Coordinates</label>
	      	<occurrences minimum="1" maximum="1"/>
	    </input>
	    <input name="azimuth" type="TextLine">
  			<label>Direction in degrees (0-359)</label>
	      	<occurrences minimum="0" maximum="1"/>
	      	<config>
    			<regexp>^([0-9]|[1-9][0-9]|[1-2][0-9][0-9]|3[0-5][0-9])$</regexp>
    		</config>
	    </input>
  	</config>
</part>