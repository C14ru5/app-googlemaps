<?xml version="1.0" encoding="UTF-8"?>
<site>
	<config>
		<input type="TextLine" name="apiKey">
			<label>Google Maps v3 API Key</label>
			<occurrences minimum="0" maximum="1"/>
    	</input>
    	<input type="TextLine" name="hue">
    		<label>Hue in degrees (from 0 to 359)</label>
    		<occurrences minimum="0" maximum="1"/>
    		<config>
    			<regexp>^([0-9]|[1-9][0-9]|[1-2][0-9][0-9]|3[0-5][0-9])$</regexp><!-- http://www.regular-expressions.info/numericranges.html -->
    		</config>
    	</input>
    	<!-- Default Theme -->
    	<!-- enable draggable/scrollable -->
  	</config>
</site>
